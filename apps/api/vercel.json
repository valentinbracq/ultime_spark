{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "buildCommand": "DATABASE_URL=postgresql://user:pass@localhost:5432/db pnpm build",
  "installCommand": "pnpm install"
}